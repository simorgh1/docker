{
	"Service": {
        "Name": "elastic",
        "Address": "elastic",
        "Port": 9200,
		"Tags": [ 
			"elastic" ,
			"core.local"
		],
		"Check": {
			"name": "elastic cluster health check",
			"http": "http://elasticsearch:9200/_cluster/health?wait_for_status=yellow&timeout=50s&pretty",
			"tls_skip_verify": false,
			"method": "GET",    
			"interval": "10s",
			"timeout": "1s"			
		},
		"Weights": {
			  "Passing": 10,
			  "Warning": 1
			}		
    }
}